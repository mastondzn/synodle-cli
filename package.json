{
    "name": "synopsis-bp",
    "version": "0.0.0",
    "type": "module",
    "engines": {
        "node": ">= 16.13 <17"
    },
    "author": {
        "email": "MastonDZN@users.noreply.github.com",
        "name": "MastonDZN",
        "url": "https://github.com/MastonDZN"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MastonDZN/synopsis-bp"
    },
    "bugs": {
        "url": "https://github.com/MastonDZN/synopsis-bp/issues"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.22",
        "@typescript-eslint/eslint-plugin": "^4.0.1",
        "@typescript-eslint/parser": "^5.16.0",
        "cspell": "^5.19.2",
        "eslint": "^7.12.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^26.1.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "jest": "^27.5.1",
        "prettier": "^2.6.0",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.21",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.7.0",
        "nodemon": "^2.0.15",
        "tsutils": "^3.21.0",
        "typescript": "*"
    },
    "scripts": {
        "start": "node --experimental-specifier-resolution=node --loader ts-node/esm --no-warnings src/main.ts",
        "start:r": "nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm --no-warnings' src/main.ts",
        "start:prod": "node ./build/src/main.js",
        "playground": "ts-node-dev ./src/playground/playground.ts",
        "build": "tsc -p tsconfig.json",
        "test": "jest --verbose --watchAll",
        "coverage": "jest --coverage",
        "typecheck": "tsc --noEmit",
        "lint": "eslint . --ext .ts",
        "lint:fix": "yarn lint --fix",
        "format": "prettier --list-different ./**/*.{js,jsx,ts,tsx,json,md,yaml}",
        "format:fix": "prettier --write ./**/*.{js,jsx,ts,tsx,json,md,yaml}",
        "clean": "rimraf coverage build tmp",
        "build:watch": "tsc -w -p tsconfig.json",
        "test:watch": "jest --watch"
    },
    "volta": {
        "node": "16.13.0"
    }
}
